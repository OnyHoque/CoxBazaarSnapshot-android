<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#383943"
    tools:context=".contact_list">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <ImageView
            android:id="@+id/imageView30"
            android:layout_width="343dp"
            android:layout_height="70dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="15dp"
            app:srcCompat="@drawable/group_58" />

        <ImageView
            android:id="@+id/imageView31"
            android:layout_width="300dp"
            android:layout_height="100dp"
            android:layout_below="@+id/imageView30"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="50dp"
            app:srcCompat="@drawable/group_89" />

        <ImageView
            android:id="@+id/imageView32"
            android:layout_width="300dp"
            android:layout_height="100dp"
            android:layout_below="@+id/imageView31"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="20dp"
            android:layout_marginTop="20dp"
            app:srcCompat="@drawable/group_88" />

        <ImageView
            android:id="@+id/imageView33"
            android:layout_width="300dp"
            android:layout_height="100dp"
            android:layout_below="@+id/imageView32"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="0dp"
            app:srcCompat="@drawable/group_91" />
    </RelativeLayout>

</ScrollView>